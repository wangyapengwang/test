        <omgdi:waypoint x="335.0" y="138.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow5" id="BPMNEdge_flow5">
        <omgdi:waypoint x="440.0" y="138.0"></omgdi:waypoint>
        <omgdi:waypoint x="485.0" y="139.0"></omgdi:waypoint>
